<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>QKA_Restaurant</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="~/Areas/Admin/Data/DesignForAdmin/assets/vendors/feather/feather.css">
    <link rel="stylesheet" href="~/Areas/Admin/Data/DesignForAdmin/assets/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Data/DesignForAdmin/assets/vendors/ti-icons/css/themify-icons.css">
    <link rel="stylesheet" href="~/Areas/Admin/Data/DesignForAdmin/assets/vendors/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Data/DesignForAdmin/assets/vendors/typicons/typicons.css">
    <link rel="stylesheet" href="~/Areas/Admin/Data/DesignForAdmin/assets/vendors/simple-line-icons/css/simple-line-icons.css">
    <link rel="stylesheet" href="~/Areas/Admin/Data/DesignForAdmin/assets/vendors/css/vendor.bundle.base.css">
    <link rel="stylesheet" href="~/Areas/Admin/Data/DesignForAdmin/assets/vendors/bootstrap-datepicker/bootstrap-datepicker.min.css">
    <!-- SweetAlert2 CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/sweetalert2@11.4.4/dist/sweetalert2.min.css">

    <!-- endinject -->
    <!-- Plugin css for this page -->
    <link rel="stylesheet" href="~/Areas/Admin/Data/DesignForAdmin/assets/vendors/datatables.net-bs4/dataTables.bootstrap4.css">
    <link rel="stylesheet" type="text/css" href="~/Areas/Admin/Data/DesignForAdmin/assets/js/select.dataTables.min.css">
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <link rel="stylesheet" href="~/Areas/Admin/Data/DesignForAdmin/assets/css/style.css">
    <!-- endinject -->
    <link rel="shortcut icon" href="~/Areas/Admin/Data/DesignForAdmin/assets/images/favicon.png" />
</head>
<body>
    <div class="container-scroller">
        <!-- partial:partials/_navbar.html -->
        <nav class="navbar default-layout col-lg-12 col-12 p-0 fixed-top d-flex align-items-top flex-row">
            <div class="text-center navbar-brand-wrapper d-flex align-items-center justify-content-start">
                <div class="me-3">
                    <button class="navbar-toggler navbar-toggler align-self-center" type="button" data-bs-toggle="minimize">
                        <span class="icon-menu">

                        </span>
                    </button>
                </div>
                <div>
                    <a class="navbar-brand brand-logo" href="@Url.Action("Index", "Home")">
                        <img src="~/Areas/Admin/Data/DesignForAdmin/assets/images/QKA_RESTAURANT_LOGO.png" style="width:400px; height:auto" alt="logo" />
                    </a>
                    <a class="navbar-brand brand-logo-mini" href="@Url.Action("Index", "Home")">
                        <img src="~/Areas/Admin/Data/DesignForAdmin/assets/images/QKA_RESTAURANT_LOGO.png" style="width:400px; height:auto" alt="logo" />
                    </a>
                </div>
            </div>
            <div class="navbar-menu-wrapper d-flex align-items-top">
                <ul class="navbar-nav">
                    <li class="nav-item fw-semibold d-none d-lg-block ms-0">
                        <h1 class="text" style="font-size:medium">Xin chào, <span class="text-success fw-bold">@Session["UserName"]</span></h1>
                        <h3 class="text" style="font-size:medium">Chúc bạn có một ngày làm việc vui vẻ!</h3>
                        <h3 class="text" style="font-size:medium">Bạn đang đăng nhập dưới quyền nhân viên kho!</h3>
                    </li>
                </ul>
                <ul class="navbar-nav ms-auto">
                    <li class="nav-item dropdown">
                        <a class="nav-link count-indicator" id="notificationDropdown" href="#" data-bs-toggle="dropdown">
                            <i class="icon-bell"></i>
                            <span class="count"></span>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right navbar-dropdown preview-list pb-0" aria-labelledby="notificationDropdown">
                            <a class="dropdown-item py-3 border-bottom">
                                <p class="mb-0 fw-medium float-start">You have 4 new notifications </p>
                                <span class="badge badge-pill badge-primary float-end">View all</span>
                            </a>
                            <a class="dropdown-item preview-item py-3">
                                <div class="preview-thumbnail">
                                    <i class="mdi mdi-alert m-auto text-primary"></i>
                                </div>
                                <div class="preview-item-content">
                                    <h6 class="preview-subject fw-normal text-dark mb-1">Application Error</h6>
                                    <p class="fw-light small-text mb-0"> Just now </p>
                                </div>
                            </a>
                            <a class="dropdown-item preview-item py-3">
                                <div class="preview-thumbnail">
                                    <i class="mdi mdi-lock-outline m-auto text-primary"></i>
                                </div>
                                <div class="preview-item-content">
                                    <h6 class="preview-subject fw-normal text-dark mb-1">Settings</h6>
                                    <p class="fw-light small-text mb-0"> Private message </p>
                                </div>
                            </a>
                            <a class="dropdown-item preview-item py-3">
                                <div class="preview-thumbnail">
                                    <i class="mdi mdi-airballoon m-auto text-primary"></i>
                                </div>
                                <div class="preview-item-content">
                                    <h6 class="preview-subject fw-normal text-dark mb-1">New user registration</h6>
                                    <p class="fw-light small-text mb-0"> 2 days ago </p>
                                </div>
                            </a>
                        </div>
                    </li>
                    <li class="nav-item dropdown d-none d-lg-block user-dropdown">
                        <a class="nav-link" id="UserDropdown" href="#" data-bs-toggle="dropdown" aria-expanded="false">
                            <img class="img-xs rounded-circle" src="@Url.Content("~/Content/Staffs/" + Session["Img"])" alt="Profile image">
                        </a>
                        <div class="dropdown-menu dropdown-menu-right navbar-dropdown" aria-labelledby="UserDropdown">
                            <div class="dropdown-header text-center">
                                <img class="img-md rounded-circle" src="@Url.Content("~/Content/Staffs/" + Session["Img"])" alt="Profile image">
                                <p class="mb-1 mt-3 fw-semibold">@Session["UserName"]</p>
                                <p class="fw-light text-muted mb-0">@Session["Email"].ToString()</p>
                            </div>
                            <a href="#" class="dropdown-item"><i class="dropdown-item-icon mdi mdi-account-outline text-primary me-2"></i> Thông Tin <span class="badge badge-pill badge-danger">1</span></a>
                            <a class="dropdown-item"><i class="dropdown-item-icon mdi mdi-message-text-outline text-primary me-2"></i> Tin Nhắn</a>
                            <a href="@Url.Action("Logout", "User", new { Area = "User"})" class="dropdown-item"><i class="dropdown-item-icon mdi mdi-power text-primary me-2"></i>Đăng Xuất</a>
                        </div>
                    </li>
                </ul>
                <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-bs-toggle="offcanvas">
                    <span class="mdi mdi-menu"></span>
                </button>
            </div>
        </nav>
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_sidebar.html -->
            <nav class="sidebar sidebar-offcanvas" id="sidebar" style="background-color: #EEEEEE; margin-right:10px;">
                <ul class="nav">
                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("Index", "HomeWareHouse", new { area = "StaffWareHouse" })">
                            <i class="mdi mdi-home-silo menu-icon"></i>
                            <span class="menu-title">Quản lý kho</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("Index", "Supplier", new { area = "StaffWareHouse" })">
                            <i class="mdi mdi-card-account-mail menu-icon"></i>
                            <span class="menu-title">Quản lý nhà cung cấp</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("Index", "Transaction", new { area = "StaffWareHouse" })">
                            <i class="mdi mdi-chart-box-plus-outline menu-icon"></i>
                            <span class="menu-title">Quản lý nhập kho</span>
                        </a>
                    </li>
                </ul>
            </nav>
            <!-- partial -->
            <div class="w-100">
                @RenderBody()
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>

    <!-- plugins:js -->
    <script src="~/Areas/Admin/Data/DesignForAdmin/assets/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/Areas/Admin/Data/DesignForAdmin/assets/vendors/bootstrap-datepicker/bootstrap-datepicker.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    <script src="~/Areas/Admin/Data/DesignForAdmin/assets/vendors/chart.js/chart.umd.js"></script>
    <script src="~/Areas/Admin/Data/DesignForAdmin/assets/vendors/progressbar.js/progressbar.min.js"></script>
    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="~/Areas/Admin/Data/DesignForAdmin/assets/js/off-canvas.js"></script>
    <script src="~/Areas/Admin/Data/DesignForAdmin/assets/js/template.js"></script>
    <script src="~/Areas/Admin/Data/DesignForAdmin/assets/js/settings.js"></script>
    <script src="~/Areas/Admin/Data/DesignForAdmin/assets/js/hoverable-collapse.js"></script>
    <script src="~/Areas/Admin/Data/DesignForAdmin/assets/js/todolist.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <script src="~/Areas/Admin/Data/DesignForAdmin/assets/js/jquery.cookie.js" type="text/javascript"></script>
    <script src="~/Areas/Admin/Data/DesignForAdmin/assets/js/dashboard.js"></script>
    <script src="~/Areas/Admin/Data/DesignForAdmin/assets/js/Chart.roundedBarCharts.js"></script>
    <!-- SweetAlert2 JS -->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <!-- Custom js for modal Index page-->
    <script src="~/Areas/StaffWareHouse/Data/DesignForStaffWareHouse/js/logicmodalmaterial.js"></script>
    <script src="~/Areas/StaffWareHouse/Data/DesignForStaffWareHouse/js/logicmodalsupplier.js"></script>
    <script src="~/Areas/StaffWareHouse/Data/DesignForStaffWareHouse/js/logicmodaltransaction.js"></script>
    <!-- End custom js for this page-->
</body>
</html>
